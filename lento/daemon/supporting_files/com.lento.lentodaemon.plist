<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.lento.lentodaemon</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/__lentodaemon__.dist/lentodaemon</string>
        <string>1</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/tmp/lentodaemon_error.log</string>
    <key>StandardOutPath</key>
    <string>/tmp/lentodaemon_console.log</string>
</dict>
</plist>